{
    "name": "Accessibility Statement Viewer",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "An extension to access a web page's accessibility statement, feedback and complaints processes",
    "icons": {
        "16": "icons/ally16x16.png",
        "32": "icons/ally32x32.png",
        "48": "icons/ally48x48.png",
        "128": "icons/ally128x128.png"
    },
    "action": {
        "default_popup": "html/popup.html"
    },
    "permissions": [
      "tabs"
    ], 
    "content_scripts": [
        {
          "js": ["scripts/content.js"],
          "matches": [
            "<all_urls>"
          ]
        }
      ]
}


